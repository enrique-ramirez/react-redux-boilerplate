{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "React and redux starter pack",
  "author": "Enrique RamiÃÅrez <me@enrique-ramirez.com>",
  "private": true,
  "license": "UNLICENSED",
  "homepage": "https://github.com/enrique-ramirez/react-redux-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/enrique-ramirez/react-redux-boilerplate/issues"
  },
  "main": "index.js",
  "directories": {
    "lib": "./src"
  },
  "scripts": {
    "prebuild": "rimraf dist && npm run lint && npm run test",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --config config/webpack/webpack.dev.js",
    "start:storybook": "start-storybook -p 9001 -c .storybook",
    "start:server": "node server.js",
    "lint": "eslint src/**/*.js && stylelint src/**/*.css",
    "test": "jest --config=config/jest/jest.config.js",
    "test:watch": "npm run test --watch",
    "locales:extract": "babel-node ./scripts/translations/extract.js",
    "update-npm-versions": "npm-check --skip-unused --update"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "src/**/*.css": "stylelint",
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:enrique-ramirez/react-redux-boilerplate.git"
  },
  "keywords": [
    "React",
    "Redux",
    "boilerplate",
    "starter"
  ],
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=8.9.3"
  },
  "dependencies": {
    "connected-react-router": "^6.8.0",
    "file-loader": "^6.0.0",
    "flexboxgrid2": "7.2.1",
    "history": "^4.10.1",
    "immutable": "4.0.0-rc.12",
    "normalizr": "^3.6.0",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-immutable-proptypes": "^2.2.0",
    "react-intl": "^4.5.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-actions": "2.6.5",
    "redux-define": "1.1.1",
    "redux-form": "^8.3.5",
    "redux-immutable": "4.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "4.0.0",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "axios": "^0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-intl": "^7.5.1",
    "babel-plugin-react-intl-auto": "^3.3.0",
    "babel-plugin-react-intl-id-hash": "2.0.0",
    "blueimp-md5": "^2.13.0",
    "body-parser": "^1.19.0",
    "circular-dependency-plugin": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-formatjs": "^2.1.5",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-redux-saga": "^1.1.3",
    "eslint-plugin-reselect": "6.6.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.4.0",
    "jest-immutable-matchers": "2.0.1",
    "lint-staged": "^10.1.7",
    "npm-check": "^5.9.2",
    "postcss": "^7.0.27",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.7.0",
    "raf": "3.4.1",
    "react-hot-loader": "^4.12.20",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "storybook-addon-intl": "2.4.1",
    "style-loader": "^1.1.4",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test",
      "pre-commit": "lint-staged"
    }
  }
}
